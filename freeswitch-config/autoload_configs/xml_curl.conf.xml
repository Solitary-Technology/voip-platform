<configuration name="xml_curl.conf" description="cURL XML Gateway">
  <bindings>
    <binding name="directory">
      <param name="gateway-url" value="http://localhost:5000/freeswitch/directory" bindings="directory"/>
      <param name="disable-100-continue" value="true"/>
    </binding>
    <binding name="dialplan">
      <param name="gateway-url" value="http://localhost:5000/freeswitch/dialplan" bindings="dialplan"/>
      <param name="disable-100-continue" value="true"/>
    </binding>
  </bindings>
</configuration>